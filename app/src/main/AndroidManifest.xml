        </activity>
        <activity android:name="com.creativeoffice.Login.LoginActivity" />
        <activity android:name="com.creativeoffice.Profile.ProfileSettingActivity" />
        <activity android:name="com.creativeoffice.Share.ShareActivity" />
        <activity android:name="com.creativeoffice.Search.SearchActivity" />
